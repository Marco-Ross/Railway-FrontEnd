<Window x:Class="Railway_FrontEnd.UpdateUserPopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Railway_FrontEnd"
        mc:Ignorable="d"
        Title="UpdateUserPopUp" Height="300" Width="300">
	<Grid>
		<TextBox x:Name="UserNamerTxt" IsEnabled="False" HorizontalAlignment="Left" Height="25" Margin="28,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101"/>
		<Label Content="Train Number:" HorizontalAlignment="Left" Margin="24,60,0,0" VerticalAlignment="Top" FontSize="12"/>
		<Button x:Name="UpdateUserBtn" Content="Update User" HorizontalAlignment="Left" Margin="153,130,0,0" VerticalAlignment="Top" Width="107" Height="25" Click="UpdateScheduleBtn_Click"/>
		<TextBox x:Name="UserSNameTxt" HorizontalAlignment="Left" Height="25" Margin="159,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101"/>
		<Label Content="Seats Booked:" HorizontalAlignment="Left" Margin="155,60,0,0" VerticalAlignment="Top"/>

	</Grid>
</Window>
